<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" maxHeight="600.0" maxWidth="550.0" minHeight="600.0" minWidth="550.0" prefHeight="600.0" prefWidth="550.0" styleClass="bg" stylesheets="@CSS/Home.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.Controller.HomeController">
   <children>
      <VBox alignment="CENTER" layoutX="150.0" layoutY="138.0" maxHeight="324.0" maxWidth="250.0" minHeight="324.0" minWidth="250.0" prefHeight="324.0" prefWidth="250.0" spacing="45.0">
         <children>
            <Button fx:id="playNew" mnemonicParsing="false" onAction="#playNewGame" text="Chơi Mới">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="text" />
               </styleClass>
            </Button>
            <Button fx:id="playContinue" mnemonicParsing="false" onAction="#playContinueGame" text="Chơi Tiếp">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="text" />
               </styleClass>
            </Button>
            <Button fx:id="p" mnemonicParsing="false" onAction="#exit" text="Thoát">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="text" />
               </styleClass>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
